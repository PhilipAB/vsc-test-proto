{
	"name": "vscprototype",
	"displayName": "VSCprototype",
	"description": "Prototype for lecturer component",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:sidebar",
		"onCommand:vscprototype.helloWorld",
		"onCommand:vscprototype.askQuestion",
		"onCommand:vscprototype.coursePanel",
		"onCommand:vscprototype.authenticate",
		"onCommand:vscprototype.refresh"
	],
	"main": "./dist/extension/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "override.background",
				"description": "In case we want to change the table's default cell color",
				"defaults": {
					"dark": "#1e1e1e",
					"light": "#FFFFFF",
					"highContrast": "editor.background"
				}
			},
			{
				"id": "override.selection.background",
				"description": "In case we want to override the table's default cell color on selection",
				"defaults": {
					"dark": "#264f78",
					"light": "#add6ff",
					"highContrast": "editor.selectionBackground"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "sidebar-view",
					"title": "QuickAccess",
					"icon": "media/quick-access.svg"
				}
			]
		},
		"views": {
			"sidebar-view": [
				{
					"type": "webview",
					"id": "sidebar",
					"name": "QuickAccess",
					"icon": "media/quick-access.svg",
					"contextualTitle": "QuickAccess"
				}
			]
		},
		"commands": [
			{
				"command": "vscprototype.helloWorld",
				"category": "VSC prototype",
				"title": "Hello World"
			},
			{
				"command": "vscprototype.askQuestion",
				"category": "VSC prototype",
				"title": "Ask Question"
			},
			{
				"command": "vscprototype.authenticate",
				"category": "VSC prototype",
				"title": "Authenticate"
			},
			{
				"command": "vscprototype.refresh",
				"category": "VSC prototype",
				"title": "Refresh"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/glob": "^7.1.3",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.10",
		"@types/polka": "^0.5.2",
		"@types/react-table": "^7.7.1",
		"@types/vscode": "^1.53.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^5.1.1",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"npm-run-all": "^4.1.5",
		"process": "^0.11.10",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"svg-url-loader": "^7.1.1",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"-": "0.0.1",
		"@material/top-app-bar": "^10.0.0",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.5",
		"@types/classnames": "^2.2.11",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"@types/react-router-dom": "^5.1.7",
		"buffer": "^6.0.3",
		"classnames": "^2.2.6",
		"crypto-browserify": "^3.12.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"node-fetch": "^2.6.1",
		"polka": "^0.5.2",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"react-table": "^7.7.0",
		"save-dev": "0.0.1-security",
		"stream-browserify": "^3.0.0",
		"util": "^0.12.3",
		"web-vitals": "^1.1.0"
	},
	"extensionDependencies": []
}